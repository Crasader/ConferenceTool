.dot{width: 78rpx;height: 24rpx;position: absolute;bottom: 30rpx;left: 50%;transform: translate(-50%,0);}
.dot icon{width: 24rpx;height: 24rpx;border-radius: 50%;background: rgba(255, 255, 255, .3);vertical-align: top;}
.dot icon:first-child{margin-right: 30rpx;}
.dot .on{background: rgba(255, 255, 255, 1)}
.click-into{width: 200rpx;height: 74rpx;position: absolute;bottom: 30rpx;right: 30rpx;}

