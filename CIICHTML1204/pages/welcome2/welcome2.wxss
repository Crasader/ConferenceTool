.animate .step image{opacity: 0;transition: all .6s;}
.animate .step image:nth-child(1){transform: translate(50rpx,0);}
.animate .step image:nth-child(2){transform: translate(50rpx,0);transition-delay: .3s;}
.animate .step image:nth-child(3){transform: translate(50rpx,0);transition-delay: .6s;}
.animate .step image:nth-child(4){transform: translate(50rpx,0) rotate(-30deg);transition-delay: .9s;}
.animate .step image:nth-child(5){transform: translate(0,-50rpx) rotate(-70deg);transition-delay: 1.2s;}
.animate .step image:nth-child(6){transform: translate(0,-50rpx) rotate(-90deg);transition-delay: 1.5s;}
.animate .step image:nth-child(7){transform: translate(0,-50rpx) rotate(-90deg);transition-delay: 1.8s;}

.animate .map, .animate .map .step-1, .animate .map .step-2{opacity: 0;transition: all .6s;}
.animate .active1{transform: translate(0,-50rpx);transition-delay: 2.1s;}
.animate .active1 .step-1{transform: translate(50rpx,0);transition-delay: 2.4s;}
.animate .active1 .step-2{transform: translate(50rpx,0);transition-delay: 2.7s;}

.animate .active2{transform: translate(-50rpx,0);transition-delay: 3.0s;}
.animate .active2 .step-1{transform: translate(0,-50rpx) rotate(-90deg);transition-delay: 3.3s;}
.animate .active2 .step-2{transform: translate(0,-50rpx) rotate(-90deg);transition-delay: 3.6s;}

.animate .active3{transform: translate(-50rpx,0);transition-delay: 3.9s;}
.animate .active3 .step-1{transform: translate(-50rpx,0) rotate(-180deg);transition-delay: 4.2s;}
.animate .active3 .step-2{transform: translate(-50rpx,0) rotate(-180deg);transition-delay: 4.5s;}

.animate .active4{transform: translate(50rpx,0);transition-delay: 4.8s;}
.animate .active4 .step-1{transform: translate(0,-50rpx) rotate(-90deg);transition-delay: 5.1s;}
.animate .active4 .step-2{transform: translate(0,-50rpx) rotate(-90deg);transition-delay: 5.4s;}

.animate .active5{transform: translate(50rpx,0);transition-delay: 5.7s;}
.animate .active5 .step-1{transform: translate(50rpx,0) rotate(-40deg);transition-delay: 6.0s;}
.animate .active5 .step-2{transform: translate(50rpx,0) rotate(-40deg);transition-delay: 6.3s;}

.animate .active6{transform: translate(50rpx,0);transition-delay: 6.6s;}


.animate.active .step image:nth-child(1), .animate.active .step image:nth-child(2), .animate.active .step image:nth-child(3){transform: translate(0,0);opacity: 1;}
.animate.active .step image:nth-child(4){transform: translate(0,0) rotate(-30deg);opacity: 1;}
.animate.active .step image:nth-child(5){transform: translate(0,0) rotate(-70deg);opacity: 1;}
.animate.active .step image:nth-child(6){transform: translate(0,0) rotate(-90deg);opacity: 1;}
.animate.active .step image:nth-child(7){transform: translate(0,0) rotate(-90deg);opacity: 1;}

.animate.active .active1, .animate.active .active1 .step-1, .animate.active .active1 .step-2{transform: translate(0,0);opacity: 1;}
.animate.active .active2, .animate.active .active3, .animate.active .active4, .animate.active .active5, .animate.active .active6{transform: translate(0,0);opacity: 1;}
.animate.active .active2 .step-1, .animate.active .active2 .step-2{transform: translate(0,0) rotate(-90deg);opacity: 1;}

.animate.active .active3 .step-1, .animate.active .active3 .step-2{transform: translate(0,0)  rotate(-180deg);opacity: 1;}

.animate.active .active4 .step-1, .animate.active .active4 .step-2{transform: translate(0,0) rotate(-90deg);opacity: 1;}

.animate.active .active5 .step-1, .animate.active .active5 .step-2{transform: translate(0,0) rotate(-40deg);opacity: 1;}




