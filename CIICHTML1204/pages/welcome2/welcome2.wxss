/* pages/welcome2/welcome2.wxss */
page{
   height: 100%;
}
.step{position: absolute;width: 50%;height: 200rpx;right: 0;top: 0;}
.step image, .step-1, .step-2{width: 45rpx;height: 18rpx;position: absolute;}
.step image:nth-child(1){top: 15rpx;right: 0;}
.step image:nth-child(2){top: 50rpx;right: 50rpx;}
.step image:nth-child(3){top: 15rpx;right: 100rpx;}
.step image:nth-child(4){top: 50rpx;right: 160rpx;transform: rotate(-30deg);}
.step image:nth-child(5){top: 40rpx;right: 212rpx;transform: rotate(-70deg);}
.step image:nth-child(6){top: 105rpx;right: 203rpx;transform: rotate(-90deg);}
.step image:nth-child(7){top: 130rpx;right: 240rpx;transform: rotate(-90deg);}


